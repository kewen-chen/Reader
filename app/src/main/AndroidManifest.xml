<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.gdgny.androidfan.reader" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.gdgny.androidfan.reader.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.gdgny.androidfan.reader.MainActivity"
            android:label="@string/app_name"></activity>
        <activity android:name="org.gdgny.androidfan.reader.ui.StaringActivity"></activity>

        <activity android:name="org.gdgny.androidfan.reader.aboutActivity"></activity>
    </application>

</manifest>
